{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "paths": {
      "@Components/*": [
        "src/components/*"
      ],
      "@Screens/*": [
        "src/screens/*"
      ],
      "@Assets/*": [
        "assets/*"
      ],
      "@Themes/*": [
        "src/themes/*"
      ]
    }
  }
}